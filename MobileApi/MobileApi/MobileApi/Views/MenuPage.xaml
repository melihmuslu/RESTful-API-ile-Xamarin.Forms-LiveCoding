<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileApi.Views.MenuPage">
  <StackLayout BackgroundColor="#455A64">
    <StackLayout VerticalOptions="FillAndExpand">
      <StackLayout Padding="20,10" Spacing="0" Orientation="Vertical" HorizontalOptions="FillAndExpand" BackgroundColor="#455A64">
        <StackLayout Orientation="Horizontal" HorizontalOptions="FillAndExpand" Padding="5">
          <Button Text="Insert" BackgroundColor="Black" TextColor="White" x:Name="btnInsert" Clicked="onInsert"></Button>
        </StackLayout>
      </StackLayout>
    </StackLayout>
  </StackLayout>
</ContentPage>